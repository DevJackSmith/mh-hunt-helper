{
    "manifest_version": 2,

    "name": "MH Hunt Helper",
    "description": "This extension tracks mousehunt hunts.",
    "version": "1.5",
    "icons": { "128": "icon.png" },

    "content_scripts": [{
        "matches": ["https://www.mousehuntgame.com/*"],
        "js": ["inject.js"]
    }],
    
    "web_accessible_resources": ["main.js"],
    
    "background": {
        "scripts": ["update_check.js"]
    },
    
    "key": "",
    
    "update_url": "https://clients2.google.com/service/update2/crx"
}
